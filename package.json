{
    "name": "@cavos/react-native",
    "version": "0.1.2",
    "description": "Cavos SDK for React Native - Invisible crypto infrastructure with native passkeys",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "react-native": "src/index.ts",
    "exports": {
        ".": {
            "react-native": "./src/index.ts",
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "src",
        "assets"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "typecheck": "tsc --noEmit",
        "clean": "rm -rf dist"
    },
    "peerDependencies": {
        "expo-crypto": ">=14.0.0",
        "expo-secure-store": ">=13.0.0",
        "react": ">=18.0.0",
        "react-native": ">=0.72.0",
        "react-native-passkey": ">=3.3.0"
    },
    "peerDependenciesMeta": {
        "react-native-passkey": {
            "optional": false
        },
        "expo-crypto": {
            "optional": false
        },
        "expo-secure-store": {
            "optional": false
        }
    },
    "dependencies": {
        "@noble/ciphers": "^2.1.1",
        "axios": "^1.6.0",
        "starknet": "^9.2.1"
    },
    "devDependencies": {
        "@types/node": "^25.0.1",
        "@types/react": "^18.2.0",
        "expo": "^54.0.29",
        "expo-auth-session": "^7.0.10",
        "expo-crypto": "^15.0.8",
        "expo-secure-store": "^15.0.8",
        "expo-web-browser": "^15.0.10",
        "tsup": "^8.0.0",
        "typescript": "^5.3.0"
    },
    "keywords": [
        "cavos",
        "starknet",
        "wallet",
        "passkey",
        "webauthn",
        "react-native",
        "gasless",
        "account-abstraction"
    ],
    "author": "Cavos",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/cavos-xyz/cavos-sdk"
    },
    "homepage": "https://cavos.xyz"
}
