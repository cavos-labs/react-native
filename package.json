{
    "name": "@cavos/react-native",
    "version": "0.1.0",
    "description": "Cavos SDK for React Native - Invisible crypto infrastructure with native passkeys",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "assets"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "typecheck": "tsc --noEmit",
        "clean": "rm -rf dist"
    },
    "peerDependencies": {
        "@react-native-async-storage/async-storage": ">=1.18.0",
        "react": ">=18.0.0",
        "react-native": ">=0.72.0",
        "react-native-passkey": ">=2.0.0",
        "react-native-quick-crypto": ">=0.6.0"
    },
    "peerDependenciesMeta": {
        "react-native-passkey": {
            "optional": false
        },
        "react-native-quick-crypto": {
            "optional": false
        },
        "@react-native-async-storage/async-storage": {
            "optional": false
        }
    },
    "dependencies": {
        "axios": "^1.6.0",
        "starknet": "^6.23.1"
    },
    "devDependencies": {
        "@types/node": "^25.0.1",
        "@types/react": "^18.2.0",
        "tsup": "^8.0.0",
        "typescript": "^5.3.0"
    },
    "keywords": [
        "cavos",
        "starknet",
        "wallet",
        "passkey",
        "webauthn",
        "react-native",
        "gasless",
        "account-abstraction"
    ],
    "author": "Cavos",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/cavos-xyz/cavos-sdk"
    },
    "homepage": "https://cavos.xyz"
}
